    "typescript",
    "node",
    "universal-user-agent",
    "node-fetch",
    "zod",
    "zod-to-json-schema"
  ],
  "author": {
    "name": "kaiyuanxiaobing",
    "url": "https://atomgit.com/kaiyuanxiaobing"
  },
  "bugs": "https://atomgit.com/atomgit-open-source-ecosystem/atomgit-mcp-server/issues",
  "license": "MulanPSL-2.0",
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.2",
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^3.3.2",
    "universal-user-agent": "^7.0.3",
    "zod": "^3.24.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "shx": "^0.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}