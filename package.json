{
  "name": "atomgit-mcp-server",
  "version": "1.0.8",
  "type": "module",
  "description": "MCP server for using AtomGit API",
  "main": "index.js",
  "bin": {
    "atomgit-mcp-server": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://atomgit.com/atomgit-open-source-ecosystem/atomgit-mcp-server"
  },
  "homepage": "https://atomgit.com/atomgit-open-source-ecosystem/atomgit-mcp-server/blob/master/README.md",
  "scripts": {
    "generate-version": "tsx common/generate-version.ts",
    "build": "pnpm run generate-version && tsc && shx chmod +x dist/*.js"
